[uwsgi]
projectname = nbhosting
chdir = @DJANGO-ROOT@
#
module = nbhosting.wsgi:application
plugins = python3
#
pidfile = /run/uwsgi.pid
#
socket = /run/nbhosting.sock
chown-socket = nginx:nginx
uid = root
gid = root
listen.owner = root
listen.group = root
#

workers = 16
max-requests = 4000

# looks like master mode makes
# systemctl restart nbhosting
# very slow 
#master=True
